{
    "name": "dnl6c/php_framework",
    "description": "creation_framework_poo_php",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "dan6c77",
            "email": "dan6c77@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "vlucas/phpdotenv": "5.4.x-dev",
        "php-di/php-di": "dev-master",
        "symfony/http-foundation": "6.2.x-dev",
        "twig/twig": "3.x-dev"
    },
    "require-dev": {
        "symfony/var-dumper": "6.2.x-dev"
    },
    "scripts": {
        "serve": [
            "php -S localhost:8000 -d display_errors=1 -t public"
        ],
        "cache:clear": [
        "rmdir var\\cache\\dev\\twig S//Q"
        ]
    },


    "config": {
        "process-timeout": 0,
        "optimize-autoloader": true
    }
}
